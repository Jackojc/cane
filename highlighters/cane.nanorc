syntax "cane" "\.cn"

# Keywords
color red "\<pat|let|map|vel|car|cdr|len|beat|skips\>"

# Operator
color green "\\|=>|~>|@|\?|<|>|\*\*|\||&|\^|,|~|'|\+|\-|\*"

# Operators/Keywords
color brightgreen "\$|:"

# Values
color yellow "[\!\.=]"

# Grouping
color white "\(|\)"

# Litteral
color pink "[0-9]*"

# Identifier
color orange "[a-zA-Z]([a-zA-Z0-9_])*"

# comments
color lightblue "#.*$"
